  gtk_widget_show (vbox8);
  gtk_container_add (GTK_CONTAINER (infor_dialog), vbox8);
  gtk_widget_set_size_request (vbox8, 200, 123);

  lbl_warning = gtk_label_new (_("Infomation Goes Here"));
  gtk_widget_show (lbl_warning);
  gtk_box_pack_start (GTK_BOX (vbox8), lbl_warning, TRUE, TRUE, 0);
  gtk_widget_set_size_request (lbl_warning, 200, -1);
  gtk_label_set_justify (GTK_LABEL (lbl_warning), GTK_JUSTIFY_CENTER);
