  gtk_widget_set_size_request (btn_infor_ok, 60, 27);

  /* Store pointers to all widgets, for use by lookup_widget(). */
  GLADE_HOOKUP_OBJECT_NO_REF (infor_dialog, infor_dialog, "infor_dialog");
  GLADE_HOOKUP_OBJECT (infor_dialog, vbox8, "vbox8");
  GLADE_HOOKUP_OBJECT (infor_dialog, lbl_warning, "lbl_warning");
  GLADE_HOOKUP_OBJECT (infor_dialog, fixed12, "fixed12");
  GLADE_HOOKUP_OBJECT (infor_dialog, btn_infor_ok, "btn_infor_ok");

  return infor_dialog;
}

