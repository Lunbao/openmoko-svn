  GtkWidget *lbl_warning;
  GtkWidget *fixed12;
  GtkWidget *btn_infor_ok;

  infor_dialog = gtk_window_new (GTK_WINDOW_TOPLEVEL);
  gtk_widget_set_size_request (infor_dialog, 200, 123);
  gtk_window_set_title (GTK_WINDOW (infor_dialog), _("Warning!"));
  gtk_window_set_position (GTK_WINDOW (infor_dialog), GTK_WIN_POS_CENTER);
  gtk_window_set_type_hint (GTK_WINDOW (infor_dialog), GDK_WINDOW_TYPE_HINT_DIALOG);

