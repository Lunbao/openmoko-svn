  fixed12 = gtk_fixed_new ();
  gtk_widget_show (fixed12);
  gtk_box_pack_start (GTK_BOX (vbox8), fixed12, FALSE, TRUE, 0);
  gtk_widget_set_size_request (fixed12, -1, 40);

  btn_infor_ok = gtk_button_new_with_mnemonic (_("OK"));
