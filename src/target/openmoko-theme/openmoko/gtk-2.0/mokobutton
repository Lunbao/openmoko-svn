
# Normal button for stylus application
style "button"
{

  fg[NORMAL]        = "#ffffff"
  fg[ACTIVE]        = "#ffffff"

  engine "pixmap"
    {

    image
      {
        function        = BOX
        recolorable     = TRUE
        state           = NORMAL
        file            = "btn_toolbar_action.png"
        border          = { 9,9,9,9 }
        stretch         = TRUE
      }
    image
      {
        function        = BOX
        recolorable     = TRUE
        state           = PRELIGHT
        file            = "btn_toolbar_action.png"
        border          = { 9,9,9,9 }
        stretch         = TRUE
      }
    image
      {
        function        = BOX
        recolorable     = TRUE
        state           = ACTIVE
        file            = "btn_toolbar_action_p.png"
        border          = { 9,9,9,9 }
        stretch         = TRUE
      }

   }
}



# Dialer number button
style "btn_dial_pad"
{
  
  #fg[NORMAL]        = "#eee08a"
  #fg[NORMAL]        = "#000000"
  #fg[ACTIVE]        = "#000000"

  engine "pixmap"
    {
    image
      {
        function        = BOX
        recolorable     = TRUE
        state           = NORMAL
        shadow          = OUT
        file            = "btn_dial_pad.png"
        border          = { 9,9,9,9 }
        stretch         = TRUE
      }
    image
      {
        function        = BOX
        recolorable     = TRUE
        state           = PRELIGHT
        shadow          = OUT
        file            = "btn_dial_pad.png"
        border          = { 9,9,9,9 }
        stretch         = TRUE
      }
    image
      {
        function        = BOX
        recolorable     = TRUE
        state           = ACTIVE
        shadow          = IN
        file            = "btn_dial_pad_p.png"
        border          = { 9,9,9,9 }
        stretch         = TRUE
      }
   }
}


# Left menu button for stylus application
style "btn_menu_left"
{

  fg[NORMAL]        = "#ffffff" # 
  fg[ACTIVE]        = "#ffffff" # 

  #text[NORMAL]      = "#000000" # 
  #text[PRELIGHT]    = "#ffffff" # 
  #text[ACTIVE]      = "#ffffff" # 
  #text[SELECTED]    = "#ffffff" # 
  #text[INSENSITIVE] = "#696969" # 
  
  engine "pixmap"
    {
    image
      {
        function        = BOX
        recolorable     = TRUE
        file            = "btn_menu_left.png"
        border          = { 9,9,9,9 }
        stretch         = TRUE
      }

   }
}


