style "togglebutton"
{
   engine "pixmap" {
    image
      {
        function        = BOX
        recolorable     = TRUE
        shadow          = IN
        file            = "btn_toggle.png"
        border          = { 9,9,9,9 }
        stretch         = TRUE
      }
    image
      {
        function        = BOX
        recolorable     = TRUE
        shadow          = OUT
        file            = "btn_toggle.png"
        border          = { 9,9,9,9 }
        stretch         = TRUE
      }
  }
}

class "GtkToggleButton" style "togglebutton"

