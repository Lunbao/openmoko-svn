class "GtkButton" style "button"

widget "*.btn_dial_pad"             style "btn_dial_pad"
widget "*.btn_dial_pad.*"           style "label_btn_dial_pad"

widget "*.btn_menu_left"            style "btn_menu_left"
widget "*.btn_filter_menu"          style "btn_filter_menu"


widget "*.btn_dial_call"            style "btn_dial_call"
widget "*.btn_dial_call.*"          style "label_hoariness"

widget "*.btn_dial_his_del"         style "btn_dial_his_del"
widget "*.btn_dial_his_del.*"       style "label_hoariness"

